<%- include('decoupage/header') %>
<%- include('decoupage/navbar') %>
    <div class=" d-flex justify-content-center  mt-5" style="margin-bottom: 20%;">
      <form class="col-md-6  col-12 border p-3 shadow " action="createOne" method="post" >
        <h1 class="text-center text-decoration-underline" style="color: #07075c;"> <i
            class="bi bi-bookmark-heart-fill"></i> Book Categorie </h1>
        <!--! title  -->
        <label for="exampleInputEmail1" class="form-label">Title</label>
        <div class="mb-3">
          <input name="title" type="text" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp"
            placeholder="title">
        </div>
        <!-- !description -->
        <label for="floatingTextarea2">description</label>
        <div class="form-floating mb-3">
          <textarea name="description" class="form-control" placeholder="content" id="floatingTextarea2"
            style="height: 100px"></textarea>
        </div>
        <!-- !submit -->
        <div class="container d-flex justify-content-end  ">
          <!-- <button type="submit" >Add categorie</button> -->
          <input type="submit" class="btn text-white text-center px-5 fw-5 " style="background-color: #07075c;"
            value="Add categorie">
        </div>
      </form>
    </div>
